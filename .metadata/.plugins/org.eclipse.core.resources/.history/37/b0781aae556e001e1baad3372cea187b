package homework_java3.basic1;

public class Cats extends Mammals{
	
	int age;			// 수명
	int num;			// 개체수
	
	
	public Cats()	{
		
	}
	
	public Cats(int height, int weight, int age, int num)	{
		this.height = height;
		this.weight = weight;
		this.age = age;
		this.num = num;
	}
	
	public void eat()	{
		System.out.println("육식을 한다.");
	}
}
