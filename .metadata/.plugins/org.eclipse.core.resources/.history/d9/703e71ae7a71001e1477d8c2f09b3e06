package loop.question3;

import java.util.Scanner;

public class Main {

	// 3. 합
	// n이 주어졌을 때, 1부터 n까지 합을 구하는 프로그램을 작성하시오.
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		int sum;
		for (int i = 0; i <= n; i++)	{
			sum = i + n;
		}
		System.out.println();
		
	}

}
