package homework_java3.extra;

public class Emproyee {
	int eno;
	String name;
	int pay;
	int type;
	
public Emproyee()	{
		
	}
	public Emproyee (int eno, String name, int pay, int type) {
		this.eno = eno;
		this.name = name;
		this.pay = pay;
	}
	
	
	public int getEno() {
		return eno;
	}
	public void setEno(int eno) {
		this.eno = eno;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public int getPay() {
		return pay;
	}
	public void setPay(int pay) {
		this.pay = pay;
	}
	public int getType() {
		return type;
	}
	public void setType(int type) {
		this.type = type;
	}
	public void showEmployeeInfo()	{
		
	}
	
	public 
	
}
