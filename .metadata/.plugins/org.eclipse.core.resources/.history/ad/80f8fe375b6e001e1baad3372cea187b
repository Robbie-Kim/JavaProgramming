package homework_java3.basic2;

public class ChildA extends Parent {
	
	String name;
	
	public ChildA()	{
		
	}
	
	public ChildA (String name)	{
		this.name = name;
	}
	
	public void name()	{
		System.out.println("이름은 " + name + "입니다.");
	}
