package loop;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner in = new Scanner(System.in);
		int a = in.nextInt();
		int b = in.nextInt();
		int t = in.nextInt();
		
		for (int i = 0; i < t; i++) {
			
			System.out.println(a + b);
			continue;
		}
		
	}

}
