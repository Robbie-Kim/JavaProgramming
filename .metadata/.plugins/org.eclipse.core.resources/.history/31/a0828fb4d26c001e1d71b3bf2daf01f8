package homework_firstweek;

public class Advance_04 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
//		4. 다음과 같이 출력하세요.
//	    *
//	   ***
//	  *****
//	 *******
//	*********
		int i = 0;
		char j = '*';
		while (i % 2 != 0 && i < 10)	{
			i++;
			j = (char)i;
			System.out.println(j);
		}
		
		
	}

}
