package loop;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
		int inNum = sc.nextInt();
		int result;
		for (int i = 1; i <= 9; i++)	{
			result = inNum * i;
			System.out.println(inNum + " * " + i + " = " + result);
		}
	}

}
